<div class="bs-example">
    <ul id="navHeader" class="nav nav-pills">
        <li>
          <div id="logo">OneList.</div>
        </li>
        <li><%= link_to "HOME", lists_path %></li>
        <li class="dropdown">
            <a href="#" data-toggle="dropdown" class="dropdown-toggle">ACTIONS <b class="caret"></b></a>
            <ul class="dropdown-menu">
                <li><%= link_to "ADD A LIST", new_list_path %></li>
                <li><%= link_to "ADD A GROUP", new_group_path %></li>
                <li></li>
                <li></li>
                <li></li>
            </ul>
          <li class="dropdown">
              <a href="#" data-toggle="dropdown" class="dropdown-toggle">LISTS <b class="caret"></b></a>
              <ul class="dropdown-menu">
                <% @lists.each do |list| %>
                    <li><%= link_to list.name, user_list_path(list.user_id, list.id)%></li>
                  <% end %>
              </ul>

          <li class="dropdown">
              <a href="#" data-toggle="dropdown" class="dropdown-toggle">GROUPS <b class="caret"></b></a>
              <ul class="dropdown-menu">
                <% @groups.each do |group| %>
                    <li><%= link_to group.name, group_path(group.id) %></li>
                  <% end %>
                <% @members.each do |member| %>
                  <% group = Group.find_by(id: member.group_id) %>
                  <% if group != nil %>
                  <% if group.creator_id != session[:user_id] %>
                    <li><%= link_to group.name, group_path(group.id) %></li>
                  <% end %>
                <% end %>
              <% end %>
              <% if @multiLists %>
                <% @multiLists.each do |multiList| %>
                  <% group = Group.find_by(id: multiList.group_id) %>
                  <% if group != nil %>
                    <% if group.creator_id != session[:user_id] %>
                      <li><%= link_to group.name, group_path(group.id) %></li>
                    <%end%>
                  <%end%>
                <%end%>
              <%end%>
              </ul>
          <li><%= link_to "INVITES", invites_path %></li>
          <li class="pull-right"><form method="POST" action="/session">
            <input name="authenticity_token" value="<%= form_authenticity_token() %>" type="hidden">
            <input name="_method" value="DELETE" type="hidden">
            <button>LOGOUT</button>
          </form></li>
        <li class="dropdown pull-right">
            <a href="#" data-toggle="dropdown" class="dropdown-toggle">PROFILE <b class="caret"></b></a>
            <ul class="dropdown-menu">
                <li><a href="#">Another action</a></li>
                <li class="divider"></li>
                <li><a href="#">Settings</a></li>
            </ul>
        </li>
    </ul>
    <hr id="headerDivider">

<div id="main-container" class="container-fluid">
      <div class="row">

        <div id="main-content" class="col-md-10">
          <%= form_for @group do |f| %>
           <%= f.label :name %>
           <%= f.text_field :name %>
            </br>
           <%= f.label :subtitle %>
           <%= f.text_field :subtitle %>
           </br >
           <%= f.label :list_id %>
           <%= f.select :list_id, options_for_seasons, :prompt => "Please select" %>
            </br >
           <%= f.check_box :single_list %>
           <%= f.label :single_list %>
            </br>
           <%= f.check_box :multi_list %>
           <%= f.label :multi_list %>
           <%= f.hidden_field :creator_id, :value => session[:user_id] %>
           </br >
           <%= f.submit "Add Group"%>
          <% end %>
          <ul>
            <% @group.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
            <% end %>
          </ul>
        </div>



<div id="sidebar" class="col-md-2">
  <div class="well well-large">


  </div>
</div>
</div>
</div>
