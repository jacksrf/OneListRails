<% if session[:username] == "jacksrf" %>
<% @users = User.all %>
<% @users.each do |user| %>
    <div class="row">
      <div class="col-md-1"><%= user.username %></div>
      <div class="col-md-1"><%= user.first_name %></div>
      <div class="col-md-1"><%= user.last_name %></div>
      <div class="col-md-2"><%= user.email %></div>
      <div class="col-md-1"><%= link_to "edit", edit_user_path(user) %></div>
    </div>
  <% end %>
  <% else %>
  <h1>You dont have access to this!</h1>
  <%= link_to "go back", user_lists_path(session[:user_id]) %>
  <% end %>
